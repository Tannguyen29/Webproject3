{% extends 'base.html.twig' %}

{% block body %}
<div class="container col-md-8 text-center mt-3">
<table class="table table-light">
    <tr>
        <th colspan="3">
            <h1 class="text text-primary">Curriculum</h1>
        </th>
    </tr>
    <tr>
        <th class="text text-success">Subject ID</th>
        <th class="text text-success">Subject Code</th>
        <th class="text text-success">Subject Name</th>
    </tr>

{% for subject in subjects %}
    <tr>
        <td>{{ subject.id }}</td>
        <td>{{ subject.subjectcode }}</td>
        <td>{{subject.subjectname}}</td>
    </tr>
{% endfor %}
{% endblock %}